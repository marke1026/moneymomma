 <script type="text/javascript">
//SYNTAX: ddtabmenu.definemenu("tab_menu_id", integer OR "auto")
 var controller = "<%= params[:controller] %>"
 var method = "<%= params[:action] %>"
if( controller == "payments" && method== "index" ){
  ddtabmenu.definemenu("ddtabs3", 0) //initialize Tab Menu #3 with 2nd tab selected
}
if( controller == "payments" && method== "new" ){
  ddtabmenu.definemenu("ddtabs3", 0) //initialize Tab Menu #3 with 2nd tab selected
}
if( controller == "payees" && method== "index" ){
  ddtabmenu.definemenu("ddtabs3", 0) //initialize Tab Menu #3 with 2nd tab selected
}
if( controller == "deposits" && method== "new" ){
  ddtabmenu.definemenu("ddtabs3", 0) //initialize Tab Menu #3 with 2nd tab selected
}
jQuery.noConflict();

</script>
  </head>

  <body>



